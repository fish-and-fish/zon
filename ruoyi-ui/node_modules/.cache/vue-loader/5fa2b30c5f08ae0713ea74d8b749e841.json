ame页面loading控制\n    if (iframe.attachEvent) {\n      this.loading = true\n      iframe.attachEvent(\"onload\", function () {\n        _this.loading = false\n      })\n    } else {\n      this.loading = true\n      iframe.onload = function () {\n        _this.loading = false\n      }\n    }\n  }\n}\n</script>\n"]}]}